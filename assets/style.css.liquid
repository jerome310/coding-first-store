/* Page Letter Style */
@import url('https://fonts.googleapis.com/css2?family=Karla:wght@200;300;400;500;600;700;800&family=Montserrat:wght@100;200;300;400;500;600;700;800;900&display=swap');


.navbar {
    display: flex;
    align-items: center;
    justify-content: space-evenly;
    font-family: 'Karla', sans-serif;
    font-family: 'Montserrat', sans-serif;
    font-weight: 500;
}
.navbar img {
    width: 180px;
}
.nav-li {
    padding: 0px;
    text-transform: uppercase;
}

.nav-li a {
    padding: 0 15px;
    color: #000052;
    text-decoration: none;
    font-size: 16px;
}

.hero-icon {
    padding: 0 5px;
    width: 25px;
}